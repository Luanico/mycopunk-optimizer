---
layout: null
permalink: /filelist.json
---

{
  {%- assign shape_files = site.static_files | where_exp: "f", "f.path contains '/shapes/'" -%}
{%- assign paths = shape_files | map: "path" -%}
{%- assign categories = [] -%}

{%- for path in paths -%}
  {%- assign path_parts = path | split: '/' -%}
  {%- assign category = path_parts[2] -%}
  {%- assign categories = categories | append: '|' | append: category -%}
{%- endfor -%}

{%- assign categories = categories | split: '|' | shift | uniq -%}

 {% for category in categories %}
    "{{ category }}": [
      {%- assign first = true -%}
      {%- for file in shape_files -%}
        {%- if file.path contains '/shapes/' and file.path contains category -%}
          {% unless first %},{% endunless %}
          "{{ file.name }}"
          {%- assign first = false -%}
        {%- endif -%}
      {% endfor %}
    ]{% unless forloop.last %},{% endunless %}
  {% endfor %}
}